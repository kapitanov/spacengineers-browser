@model SeMapBrowser.Models.WorldUploadResultModel
<div class="container">
    @if (Model.Success)
    {
        <div class="page-header">
            <h1>
                OK!
            </h1>
        </div>

        <p>
            @Model.Message
        </p>

        <p>
            <a href="@Url.Action("ViewWorld", new {id = Model.WorldId})" class="btn btn-primary">
                Open world
            </a>
            <a href="@Url.Action("Index")" class="btn btn-default">
                Back to the main page
            </a>
        </p>
    }
    else
    {
        <div class="page-header">
            <h1>
                Failed :(
            </h1>
        </div>
        <p class="alert alert-danger">
            @Model.Message
        </p>
        <a href="@Url.Action("Index")" class="btn btn-default">
            Back to the main page
        </a>
    }
</div>